{
	"version": "2.0.0",
	"tasks": [
    {
			"type": "npm",
			"script": "build",
			"problemMatcher": "$tsc",
			"presentation": {
				"reveal": "silent",
        "clear": true
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
    {
			"type": "shell",
			"command": "nvs use 20; npm run build",
			"presentation": {
				"reveal": "silent",
        "clear": true
			},
			"group": "build",
      "label": "nvs: build"
		}
	]
}
